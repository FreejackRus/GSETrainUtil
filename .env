# Порты на хосте (можно поменять под любую свободную пару)
BACKEND_PORT=3010
FRONTEND_PORT=8100

# CORS (тот адрес/порт, с которого браузер будет лезть к API)
# Если вы меняете FRONTEND_PORT — поправьте и здесь
CLIENT_URL=http://localhost

# Prisma / БД (имя сервиса db внутри сети Compose)
DATABASE_URL=postgresql://gse_user:password@db:5432/gse_train_util?schema=public

# JWT secret
JWT_SECRET=your-secret-key-here